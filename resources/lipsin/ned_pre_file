import osgNodes.ChannelController;
import osgNodes.GroundCommNode;
import osgNodes.OsgEarthScene;
import osgNodes.SatComm;
import channels.SatToSat;
import channels.SatToGround;
import inet.projects.lipsin.lipsin_node.LipsinSatellite;
import inet.networklayer.configurator.ipv4.Ipv4NetworkConfigurator;
import inet.projects.lipsin.lipsin_global_recorder.LipsinGlobalRecorder;
network OsgEarthNet
{
	parameters:
		int rngNum = default(0);
		double sendInterval = default(1);
	types:
		channel SatToSat_10Mbps extends SatToSat
		{
			datarate=10Mbps;
		}
		channel SatToSat_100Mbps extends SatToSat
		{
			datarate=100Mbps;
		}
		channel SatToSat_1Gbps extends SatToSat
		{
			datarate=1Gbps;
		}
		channel SatToGround_10Mbps extends SatToGround
		{
			datarate=10Mbps;
		}
		channel SatToGround_100Mbps extends SatToGround
		{
			datarate=100Mbps;
		}
		channel SatToGround_1Gbps extends SatToGround
		{
			datarate=1Gbps;
		}
	submodules:
		osgEarthScene: OsgEarthScene {
		}
		channelController: ChannelController {
			parameters:
				config = xmldoc("./channel.xml");
		}
		configurator: Ipv4NetworkConfigurator {
            parameters:
                config = xml("<config><interface among='**' address='10.x.x.x' netmask='255.255.255.252'/></config>");
                addStaticRoutes = false;
                addDefaultRoutes = false;
        }
