import nedFiles.osgNodes.ChannelController;
import nedFiles.osgNodes.GroundCommNode;
import nedFiles.osgNodes.OsgEarthScene;
import nedFiles.osgNodes.SatComm;
import nedFiles.channels.SatToSat;
import nedFiles.channels.SatToGround;
import inet.common.scenario.ScenarioManager;
import inet.networklayer.configurator.ipv4.Ipv4NetworkConfigurator;
import nedFiles.osgNodes.OspfSatellite;
import inet.projects.generic_tools.global_module.GlobalModule;
network OsgEarthNet
{
	parameters:
		int rngNum = default(0);
		double sendInterval = default(1);
	types:
		channel SatToSat_10Mbps extends SatToSat
		{
			datarate=10Mbps;
		}
		channel SatToSat_100Mbps extends SatToSat
		{
			datarate=100Mbps;
		}
		channel SatToSat_1Gbps extends SatToSat
		{
			datarate=1Gbps;
		}
		channel SatToGround_10Mbps extends SatToGround
		{
			datarate=10Mbps;
		}
		channel SatToGround_100Mbps extends SatToGround
		{
			datarate=100Mbps;
		}
		channel SatToGround_1Gbps extends SatToGround
		{
			datarate=1Gbps;
		}
	submodules:
		osgEarthScene: OsgEarthScene {
		}
		channelController: ChannelController {
			parameters:
				config = xmldoc("./channel.xml");
		}
		configurator: Ipv4NetworkConfigurator {
			parameters:
				config = xmldoc("./address.xml");
				addStaticRoutes = false;
				addDefaultRoutes = false;
		}
		globalModule: GlobalModule {

        }
